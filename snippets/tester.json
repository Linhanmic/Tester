{
    "Configuration Block": {
        "prefix": "tset",
        "body": [
            "tset",
            "\t// 项目通道初始化",
            "\ttcaninit ${1:设备ID},${2:设备索引},${3:设备通道索引},${4:仲裁波特率},${5:数据波特率}",
            "\t// 诊断配置",
            "\ttdiagnose_rid ${6:诊断响应ID}",
            "\ttdiagnose_sid ${7:诊断请求ID}",
            "\ttdiagnose_keyk ${8:诊断密钥KKey}",
            "\t// 故障码配置",
            "\ttdiagnose_dtc ${9:故障码},${10:故障描述}",
            "tend",
            "$0"
        ],
        "description": "创建完整的配置块"
    },
    "Channel Init": {
        "prefix": "tcaninit",
        "body": [
            "tcaninit ${1:设备ID},${2:设备索引},${3:设备通道索引},${4:仲裁波特率},${5:数据波特率}"
        ],
        "description": "初始化CAN/CAN-FD通道"
    },
    "Diagnose Config": {
        "prefix": "tdiag",
        "body": [
            "tdiagnose_rid ${1:诊断响应ID}",
            "tdiagnose_sid ${2:诊断请求ID}",
            "tdiagnose_keyk ${3:诊断密钥KKey}"
        ],
        "description": "诊断配置"
    },
    "DTC Config": {
        "prefix": "tdtc",
        "body": [
            "tdiagnose_dtc ${1:故障码},${2:故障描述}"
        ],
        "description": "故障码配置"
    },
    "Test Suite": {
        "prefix": "ttitle",
        "body": [
            "ttitle=${1:测试用例集名称}",
            "\ttstart=${2:测试用例名称}",
            "\t\t$0",
            "\ttend",
            "ttitle-end"
        ],
        "description": "创建测试用例集"
    },
    "Test Case": {
        "prefix": "tstart",
        "body": [
            "tstart=${2:测试用例名称}",
            "\t$0",
            "tend"
        ],
        "description": "创建测试用例"
    },
    "CAN Send": {
        "prefix": "tcans",
        "body": [
            "tcans ${1:项目通道},${2:报文ID},${3:报文数据},${4:发送间隔(毫秒)},${5:发送次数}"
        ],
        "description": "发送CAN报文"
    },
    "CAN Receive with Check": {
        "prefix": "tcanr",
        "body": [
            "tcanr ${1:项目通道},${2:报文ID},${3:数据范围},${4:预期值},${5:超时时间(毫秒)}"
        ],
        "description": "接收并校验CAN报文"
    },
    "CAN Receive with Print": {
        "prefix": "tcanr-print",
        "body": [
            "tcanr ${1:项目通道},${2:报文ID},${3:数据范围},print"
        ],
        "description": "接收并输出CAN报文数据"
    },
    "Delay": {
        "prefix": "tdelay",
        "body": [
            "tdelay ${1:延时时间(毫秒)}"
        ],
        "description": "延时(毫秒)"
    },
    "Full Example": {
        "prefix": "ttemplate",
        "body": [
            "// Tester测试文件模板",
            "",
            "// 设备配置块",
            "tset",
            "\t// 项目通道初始化",
            "\ttcaninit ${1:设备ID},${2:设备索引},${3:设备通道索引},${4:仲裁波特率},${5:数据波特率}",
            "\n\t// 诊断配置",
            "\ttdiagnose_rid ${6:诊断响应ID}",
            "\ttdiagnose_sid ${7:诊断请求ID}",
            "\ttdiagnose_keyk ${8:诊断密钥KKey}",
            "\n\t// 故障码配置",
            "\ttdiagnose_dtc ${9:故障码},${10:故障描述}",
            "tend",
            "",
            "// 测试用例集",
            "ttitle=${10:功能测试}",
            "\t// 测试用例",
            "\ttstart=${11:基本通信测试}",
            "\t\t// 测试命令",
            "\t\ttcans ${12:项目通道},${13:报文ID},${14:报文数据},${15:发送间隔},${16:发送次数}",
            "\t\ttcanr ${17:项目通道},${18:报文ID},${19:数据范围},${20:预期值},${21:超时时间}",
            "\t\ttdelay ${22:延时时间(毫秒)}",
            "\ttend",
            "ttitle-end",
            "$0"
        ],
        "description": "完整的测试文件模板"
    },
    "Enum Definition": {
        "prefix": "tenum",
        "body": [
            "tenum ${1:枚举名} ${2:0=${3:值1}}, ${4:1=${5:值2}}, ${6:2=${7:值3}}"
        ],
        "description": "定义枚举类型"
    },
    "Bitfield Function - Single Message": {
        "prefix": "tbitfield",
        "body": [
            "tbitfield ${1:函数名} ${2:参数1}=\"${3:变量1}\", ${4:参数2}=\"${5:变量2}\":",
            "  ${6:0x100}, ${7:1.0-1.7}=\"${3}\"/${8:100}, ${9:2.0-2.7}=\"${5}\""
        ],
        "description": "定义位域函数（单CAN报文）"
    },
    "Bitfield Function - Multi Message": {
        "prefix": "tbitfield-multi",
        "body": [
            "tbitfield ${1:函数名} ${2:参数1}=\"${3:变量1}\", ${4:参数2}=\"${5:变量2}\", ${6:参数3}=\"${7:变量3}\":",
            "  ${8:0x100}, ${9:1.0-1.7}=\"${3}\"/${10:100}, ${11:2.0-2.7}=\"${5}\";",
            "  ${12:0x200}, ${13:1.0-1.7}=\"${7}\""
        ],
        "description": "定义位域函数（多CAN报文）"
    },
    "Bitfield Call": {
        "prefix": "call",
        "body": [
            "${1:函数名} ${2:参数1}=${3:值1}, ${4:参数2}=${5:值2}"
        ],
        "description": "调用位域函数"
    },
    "Confirm": {
        "prefix": "tconfirm",
        "body": [
            "tconfirm ${1:请确认操作结果}"
        ],
        "description": "用户确认"
    },
    "Bitfield Example - Vehicle Speed": {
        "prefix": "tbitfield-speed",
        "body": [
            "// 定义车速单位枚举",
            "tenum 车速单位 0=km/h, 1=mph, 2=m/s",
            "",
            "// 定义车速信号位域函数",
            "tbitfield 车速信号 车速值=\"车速\", 车速单位=\"单位\":",
            "  ${1:0x144}, ${2:1.0-2.7}=\"车速\"/${3:100}, ${4:3.0-3.1}=\"单位\"",
            "",
            "// 调用示例",
            "车速信号 车速值=${5:100}, 车速单位=${6:km/h}"
        ],
        "description": "车速信号位域函数完整示例"
    },
    "Bitfield Example - Gear Position": {
        "prefix": "tbitfield-gear",
        "body": [
            "// 定义档位枚举",
            "tenum 档位 0=P档, 1=R档, 2=N档, 3=D档, 4=S档",
            "",
            "// 定义档位信号位域函数",
            "tbitfield 档位信号 当前档位=\"档位\":",
            "  ${1:0x260}, ${2:1.0-1.2}=\"档位\"",
            "",
            "// 调用示例",
            "档位信号 当前档位=${3:D档}"
        ],
        "description": "档位信号位域函数完整示例"
    }
}